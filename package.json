{
  "name": "sample",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write --cache \"${npm_package_config_prettier_glob}\"",
    "format:check": "prettier --check --cache \"${npm_package_config_prettier_glob}\"",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "spell-check": "cspell . --no-progress --cache",
    "fixer": "pnpm lint:fix && pnpm format && pnpm type-check && pnpm spell-check",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@sentry/react": "^9.3.0",
    "@t3-oss/env-core": "^0.12.0",
    "@tailwindcss/vite": "^4.0.9",
    "@tanstack/react-query": "^5.66.11",
    "@tanstack/react-router": "^1.112.0",
    "@tanstack/react-table": "^8.21.2",
    "axios": "^1.8.1",
    "i18next": "^24.2.2",
    "lucide-react": "^0.477.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "react-scan": "^0.2.8",
    "sonner": "^2.0.1",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "^4.0.9",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.21.0",
    "@sentry/vite-plugin": "^3.2.1",
    "@storybook/addon-a11y": "^8.6.3",
    "@storybook/addon-essentials": "^8.6.3",
    "@storybook/addon-interactions": "^8.6.3",
    "@storybook/addon-onboarding": "^8.6.3",
    "@storybook/addon-viewport": "^8.6.3",
    "@storybook/blocks": "^8.6.3",
    "@storybook/preview-api": "^8.6.3",
    "@storybook/react": "^8.6.3",
    "@storybook/react-vite": "^8.6.3",
    "@storybook/test": "^8.6.3",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@tanstack/react-query-devtools": "^5.66.11",
    "@tanstack/router-devtools": "^1.112.6",
    "@tanstack/router-plugin": "^1.112.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cspell": "^8.17.5",
    "eslint": "^9.21.0",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-storybook": "^0.11.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.6.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.0",
    "vite-bundle-analyzer": "^0.17.1",
    "vitest": "^3.0.7",
    "web-vitals": "^4.2.4"
  }
}
